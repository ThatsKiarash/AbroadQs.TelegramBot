{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Bot (Local - GetUpdates)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "dashboard",
      "applicationUrl": "http://localhost:5252",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Telegram__UpdateMode": "GetUpdates"
      }
    },
    "Bot (Local + Tunnel - Webhook)": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -File \"run-with-tunnel.ps1\"",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://127.0.0.1:5252"
      }
    },
    "Bot (Dashboard only)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "dashboard",
      "applicationUrl": "http://localhost:5252",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Bot (HTTPS)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7095;http://localhost:5252",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
